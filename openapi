          type: string
        duration:
          format: int64
          type: integer
        id:
          $ref: '#/components/schemas/ObjectId'
        key:
          type: string
        lyricist:
          type: string
        meta:
          type: string
        subtitle:
          type: string
        tempo:
          format: int32
          type: integer
        text:
          type: array
          items:
            type: string
        time:
          format: date-time
          type: string
        title:
          type: string
